#include <stdio.h>
int main()
{
    int values[10];
    int sum=0;
    float mean;
    printf("Enter 10 values:\n");
    for (int i = 0; i < 10; i++)
    {
        scanf("%d", &values[i]);
        sum +=values[i];
    }
    mean = sum/10;  
    printf("Sum = %d\n", sum);
    printf("Mean = %.2f\n", mean);
    return 0;
}
